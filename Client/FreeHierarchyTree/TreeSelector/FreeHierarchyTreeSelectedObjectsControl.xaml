<visual:PopupBase  x:Class="Proryv.ElectroARM.Controls.Controls.FreeHierarchyTree.TreeSelector.FreeHierarchyTreeSelectedObjectsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             xmlns:visual="clr-namespace:Proryv.AskueARM2.Client.Visual" x:Name="Self">
    <Label x:Name="selectedCount" Height="20" Margin="5,0,0,0" Background="Transparent" BorderBrush="Transparent" 
                             Foreground="{StaticResource IconRed}" VerticalAlignment="Top" MaxWidth="100"  MinWidth="50"
                             FontWeight="Bold" Padding="3" Content="{Binding ElementName=Self, Path=Text, FallbackValue=0, TargetNullValue=0}"/>
</visual:PopupBase>
