    <!--MenuButtonStyle-->
    <Style x:Key="MenuButtonStyle" TargetType="{x:Type armControlsMenu:MenuButton}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <ControlTemplate.Resources>

                        <Storyboard x:Key="StoryboardStart">
                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="textBlock" Storyboard.TargetProperty="(TextElement.Foreground).(SolidColorBrush.Color)" Duration="00:00:00.0010000">
                                <SplineColorKeyFrame KeyTime="00:00:00" Value="#FF068EE5"/>
                            </ColorAnimationUsingKeyFrames>
                        </Storyboard>
                        <!--<Storyboard x:Key="StoryboardEnd">
                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="textBlock" Storyboard.TargetProperty="(TextElement.Foreground).(SolidColorBrush.Color)" Duration="00:00:00.0010000">
                                <SplineColorKeyFrame KeyTime="00:00:00" Value="#FF004576"/>
                            </ColorAnimationUsingKeyFrames>
                        </Storyboard>-->
                        <Storyboard x:Key="StoryboardEnd">
                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="textBlock" Storyboard.TargetProperty="(TextElement.Foreground).(SolidColorBrush.Color)" Duration="00:00:00.0010000">
                                <SplineColorKeyFrame KeyTime="00:00:00" Value="#FF34699D"/>
                            </ColorAnimationUsingKeyFrames>
                        </Storyboard>
                      
                    </ControlTemplate.Resources>
                    <StackPanel HorizontalAlignment="Stretch" Margin="0" Orientation="Horizontal" UseLayoutRounding="True" VerticalAlignment="Stretch" Background="Transparent" Height="50">
                        <Rectangle  Width="32" Height="32"  Fill="{Binding Path=Tag, RelativeSource={RelativeSource TemplatedParent}}"  VerticalAlignment="Center" Margin="8,4,15,0"></Rectangle>
                        <TextBlock x:Name="textBlock" Text="{DynamicResource caption}" TextDecorations="Underline"  FontFamily="{StaticResource BaseBoldFontFamily}" 
                                TextWrapping="Wrap" Margin="0"   HorizontalAlignment="Stretch" FontSize="16.5" Foreground="{StaticResource SelectionFontColor}" TextAlignment="Left" VerticalAlignment="Center"  MaxWidth="175">

                            <TextBlock.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform ScaleY="{StaticResource FontScaleYMenuButton}"/>
                                    <!--<ScaleTransform ScaleX="{StaticResource FontScaleXMenuButton}"/>-->
                                </TransformGroup>
                            </TextBlock.RenderTransform>
                        </TextBlock >
                    </StackPanel>
                    <ControlTemplate.Triggers>
                        <EventTrigger RoutedEvent="Mouse.MouseEnter">
                            <BeginStoryboard Storyboard="{StaticResource StoryboardStart}"/>
                        </EventTrigger>
                        <EventTrigger RoutedEvent="Mouse.MouseLeave">
                            <BeginStoryboard Storyboard="{StaticResource StoryboardEnd}"/>
                        </EventTrigger>
                        <Trigger Property="IsFocused" Value="True"/>
                        <Trigger Property="IsDefaulted" Value="True"/>
                        <Trigger Property="IsPressed" Value="True"/>
                        <Trigger Property="IsEnabled" Value="False"/>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>

        <Setter Property="FontSize" Value="12"/>
        <Setter Property="Foreground" Value="Black"/>
    </Style>


